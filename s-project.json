{
    "name": "serverless-clv-oauth",
    "custom": {
        "cors": {
            "allowOrigin": "*",
            "allowHeaders": [
                "Content-Type", "X-Amz-Date", "Authorization",
                "X-Api-Key",
                "X-CODE"
            ]
        }
    },
    "plugins": [
        "serverless-cors-plugin", 
        "serverless-offline"
    ]
}